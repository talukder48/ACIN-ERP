	<parameter name="p_branch_code" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    b.gl_code,
    l.gl_name,
    CASE WHEN b.gl_balance > 0 THEN b.gl_balance ELSE 0 END Credit,
    CASE WHEN b.gl_balance < 0 THEN -1*b.gl_balance ELSE 0 END Debit
FROM
    `as_glbalance` b
JOIN `as_glcode` l ON
    (b.gl_code = l.gl_code)
WHERE
    branch_code = '0018'
    and gl_balance<>0
    and l.gl_level='P']]>
	</queryString>
