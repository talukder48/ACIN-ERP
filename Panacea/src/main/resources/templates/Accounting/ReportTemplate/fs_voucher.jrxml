	<queryString>
		<![CDATA[SELECT
    concat( l.tran_date, l.tran_batch) tranGroup,
    l.tran_date,
    l.tran_batch,
    l.tran_remarks,
    h.glcode,
    k.gl_name,
    h.debit_amt,
    h.credit_amt,
    h.narration,
    l.enty_by,
    l.auth_by

FROM
    `as_transaction_list` l
JOIN `as_transaction` h ON
    (
        l.tran_branch = h.tran_branch AND l.tran_date = h.tran_date AND l.tran_batch = h.tran_batch
    )
JOIN `as_glcode` k
on(k.gl_code=h.glcode)
WHERE 1
ORDER BY
    l.tran_date,
    l.tran_batch]]>
	</queryString>
